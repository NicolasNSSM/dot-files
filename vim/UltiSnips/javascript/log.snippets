snippet cl
console.log(${1})
endsnippet

snippet cl|
console.log(${1}) ||
endsnippet

snippet cw
console.warn(${1})
endsnippet

snippet cw|
console.warn(${1}) ||
endsnippet

snippet stack "stack trace" !b
console.log("\n-------------- STACK TRACE: ${1}-------------\n", (new Error()).stack, "\n-------------- !STACK TRACE -------------\n");
endsnippet
